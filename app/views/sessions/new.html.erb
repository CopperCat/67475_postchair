
 <div class="container">
<div class="row">
</div>
<div class="row">
</div>
<%= form_tag sessions_path do %>
<div class="row">
 <div class="col s6 push-s3">
	<div class="control-group">
    <%= label_tag :email %>
    <div class="controls">
      <%= text_field_tag :email, params[:email] %>
      </div>
    </div>
  </div>
</div>
<div class="row">
 <div class="col s6 push-s3">
	<div class="control-group">
    <%= label_tag :password %>
    <div class="controls">
      <%= password_field_tag :password %>
      </div>
    </div>
  </div>
</div>
  <br>
<div class="row">
 <div class="col s6 push-s3">
  <div class="actions"><%= submit_tag "Log In", class: "btn btn-default blue lighten-1"%></div>
  </div>
</div>
<% end %>
 <div class="row">
  <div class="col s6 push-s3">
    <p>Don't have an account? <%= link_to 'Sign Up', new_user_path %>.</p>
    </div>
  </div>
</div>

